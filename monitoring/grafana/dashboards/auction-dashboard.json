        {
            "id": 1,
            "title": "HTTP Request Rate (req/sec)",
            "type": "timeseries",
            "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
            "datasource": {
                "type": "prometheus",
                "uid": "Prometheus"
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_total{app=\"auction-simulator\"}[5m])) by (method, route)",
                    "legendFormat": "{{method}} {{route}}",
                    "refId": "A",
                    "datasource": {
                        "type": "prometheus",
                        "uid": "Prometheus"
                    }
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "min": 0
                }
            },
            "options": {
                "legend": {
                    "displayMode": "table",
                    "placement": "bottom"
                }
            }
        },
